 
class Cancion:
    def __init__(self, titulo, autor): 
        self._titulo = titulo 
        self._autor = autor 
    
    def get_titulo(self):
        return self._titulo 
    
    def get_autor(self): 
        return self._autor 
        
    def set_titulo(self, nuevo_titulo): 
        self._titulo = nuevo_titulo 
        
    def set_autor(self, nuevo_autor):
        self.autor = nuevo_autor 
        
    def __str__(self):
        return f"'{self._titulo}' de {self._autor}"
        
c = Cancion("Stile" , "Taylor Swift") 

print(c)

c.set_titulo("Blank Space") 

print(c)
    

